---
import BaseLayout from '../../../layouts/BaseLayout.astro';
import CpgIcdCodesPage from '../../../react-pages/clinical-tools/CpgIcdCodesPage.jsx';

const basePath = import.meta.env.BASE_URL;
const locale = 'ar';

---

<BaseLayout title="[AR] ICD Codes - Research Portal" locale={locale}>
  <main class="max-w-7xl mx-auto px-4 md:px-8 py-10 min-h-[60vh]">
    <CpgIcdCodesPage client:load basePath={basePath} locale={locale} />
  </main>
</BaseLayout>
