---
import BaseLayout from '../../../layouts/BaseLayout.astro';
import TopicGuideWomenGirlsPage from '../../../react-pages/evidence-insights/TopicGuideWomenGirlsPage.jsx';

const basePath = import.meta.env.BASE_URL;
const locale = 'en';

---

<BaseLayout title="Women & Girls with ADHD - Research Portal" locale={locale}>
