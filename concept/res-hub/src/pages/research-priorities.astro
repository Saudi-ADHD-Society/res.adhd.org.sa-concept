---
import BaseLayout from '../layouts/BaseLayout.astro';
import ResearchPrioritiesPage from '../react-pages/governance/ResearchPrioritiesPage.jsx';

const basePath = import.meta.env.BASE_URL;
---

<BaseLayout title="Research Priorities - Research Portal">
  <main class="max-w-7xl mx-auto px-4 md:px-8 py-10 min-h-[60vh]">
    <ResearchPrioritiesPage client:load basePath={basePath} />
  </main>
</BaseLayout>
