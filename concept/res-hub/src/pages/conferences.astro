---
import BaseLayout from '../layouts/BaseLayout.astro';
import ConferencesPage from '../react-pages/research-projects/ConferencesPage.jsx';

const basePath = import.meta.env.BASE_URL;
---

<BaseLayout title="Conferences - Research Portal">
  <main class="max-w-7xl mx-auto px-4 md:px-8 py-10 min-h-[60vh]">
    <ConferencesPage client:load basePath={basePath} />
  </main>
</BaseLayout>

