---
import BaseLayout from '../../../layouts/BaseLayout.astro';
import CpgStrengthOfRecommendationsPage from '../../../react-pages/clinical-tools/CpgStrengthOfRecommendationsPage.jsx';

const basePath = import.meta.env.BASE_URL;
const locale = 'en';

---

<BaseLayout title="Strength of Recommendations - Research Portal" locale={locale}>
  <main class="max-w-7xl mx-auto px-4 md:px-8 py-10 min-h-[60vh]">
    <CpgStrengthOfRecommendationsPage client:load basePath={basePath} locale={locale} />
  </main>
</BaseLayout>
