---
import BaseLayout from '../layouts/BaseLayout.astro';
import PrivacyPolicyPage from '../react-pages/legal/PrivacyPolicyPage.jsx';

const basePath = import.meta.env.BASE_URL;
---

<BaseLayout title="Privacy Policy - Research Portal">
  <main class="max-w-7xl mx-auto px-4 md:px-8 py-10 min-h-[60vh]">
    <PrivacyPolicyPage client:load basePath={basePath} />
  </main>
</BaseLayout>

