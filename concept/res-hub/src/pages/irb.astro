---
import BaseLayout from '../layouts/BaseLayout.astro';
import IrbPage from '../react-pages/governance/IrbPage.jsx';

const basePath = import.meta.env.BASE_URL;
---

<BaseLayout title="Research Ethics & IRB - Research Portal">
  <main class="max-w-7xl mx-auto px-4 md:px-8 py-10 min-h-[60vh]">
    <IrbPage client:load basePath={basePath} />
  </main>
</BaseLayout>
