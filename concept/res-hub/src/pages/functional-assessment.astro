---
import BaseLayout from '../layouts/BaseLayout.astro';
import FunctionalAssessmentPage from '../react-pages/clinical-tools/FunctionalAssessmentPage.jsx';

const basePath = import.meta.env.BASE_URL;
---

<BaseLayout title="ICF & Functional Assessment - Research Portal">
  <main class="max-w-7xl mx-auto px-4 md:px-8 py-10 min-h-[60vh]">
    <FunctionalAssessmentPage client:load basePath={basePath} />
  </main>
</BaseLayout>
