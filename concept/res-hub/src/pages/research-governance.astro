---
import BaseLayout from '../layouts/BaseLayout.astro';
import ResearchGovernancePage from '../react-pages/governance/ResearchGovernancePage.jsx';

const basePath = import.meta.env.BASE_URL;
---

<BaseLayout title="Research Governance - Research Portal">
  <main class="max-w-7xl mx-auto px-4 md:px-8 py-10 min-h-[60vh]">
    <ResearchGovernancePage client:load basePath={basePath} />
  </main>
</BaseLayout>
