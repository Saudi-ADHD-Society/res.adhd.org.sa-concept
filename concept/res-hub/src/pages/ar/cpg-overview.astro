---
import BaseLayout from '../../layouts/BaseLayout.astro';
import CpgOverviewPage from '../../react-pages/research-projects/CpgOverviewPage.jsx';

const basePath = import.meta.env.BASE_URL;
const locale = 'ar';

---

<BaseLayout title="[AR] CPG Overview - Research Portal" locale={locale}>
  <main class="max-w-7xl mx-auto px-4 md:px-8 py-10 min-h-[60vh]">
    <CpgOverviewPage client:load basePath={basePath} locale={locale} />
  </main>
</BaseLayout>
